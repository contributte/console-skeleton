{
  "name": "contributte/console-skeleton",
  "description": "Console (CLI) project skeleton based on Nette Framework (@nette),  Symfony (@symfony) and Contributte (@contributte) libraries by @f3l1x.",
  "keywords": [
    "php",
    "nette",
    "contributte",
    "sandbox",
    "project",
    "skeleton",
    "cli"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.4",

    "contributte/bootstrap": "^0.5.0",
    "contributte/di": "^0.5.0",
    "contributte/utils": "^0.5.0",
    "contributte/latte": "^0.5.0",
    "contributte/tracy": "^0.5.0",
    "contributte/console": "^0.9.0",

    "nette/robot-loader": "^3.4.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.13",
    "ninjify/nunjuck": "^0.4",
    "contributte/dev": "^0.3.0",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-doctrine": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\Commands\\": "app/commands"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
