{
  "name": "planette/cindy-project",
  "description": "Cindy Project is a CLI starter kit project for Nette Framework with Symfony Console and many useful libraries.",
  "keywords": [
    "php",
    "nette",
    "planette",
    "contributte",
    "sandbox",
    "project",
    "web"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.2",
    "contributte/bootstrap": "^0.4.0",
    "contributte/di": "^0.4.0",
    "contributte/utils": "^0.4.1",
    "contributte/latte": "^0.4.0",
    "contributte/tracy": "^0.4.1",
    "contributte/console": "^0.7.1",
    "nette/robot-loader": "~3.2.0"
  },
  "require-dev": {
		"ninjify/qa": "^0.10.0",
		"ninjify/nunjuck": "^0.3.0",
		"contributte/dev": "^0.2.0",
		"phpstan/phpstan": "^0.12.0",
		"phpstan/phpstan-doctrine": "^0.12.0",
		"phpstan/phpstan-deprecation-rules": "^0.12.0",
		"phpstan/phpstan-nette": "^0.12.0",
		"phpstan/phpstan-phpunit": "^0.12.0",
		"phpstan/phpstan-strict-rules": "^0.12.0"
  },
  "autoload": {
    "psr-4": {
      "App\\Commands\\": "app/commands"
    }
  },
	"prefer-stable": true,
	"minimum-stability": "dev",
  "config": {
    "platform": {
      "php": "7.3"
    }
  }
}
